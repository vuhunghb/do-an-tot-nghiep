<?xml version="1.0"?>

<faces-config
    version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
>
	<managed-bean>
		<managed-bean-name>dishBeans</managed-bean-name>
		<managed-bean-class>irestads.beans.dishs_store.DishBeans</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>categoryBeans</managed-bean-name>
		<managed-bean-class>irestads.beans.dishs_store.CategoryBeans</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>fileUploadController</managed-bean-name>
		<managed-bean-class>irestads.beans.dishs_store.FileUploadController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menuBeans</managed-bean-name>
		<managed-bean-class>irestads.beans.maintain_menu.MenuBeans</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderBeans</managed-bean-name>
		<managed-bean-class>irestads.beans.order.OrderBeans</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adsItemBeans</managed-bean-name>
		<managed-bean-class>irestads.beans.ads.AdsItemBeans</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>categoryAdsBeans</managed-bean-name>
		<managed-bean-class>irestads.beans.ads.CategoryAdsBeans</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>views/index.xhtml</display-name>
		<from-view-id>/views/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToMenu</from-outcome>
			<to-view-id>/views/menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/index.xhtml</display-name>
		<from-view-id>/views/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToOrder</from-outcome>
			<to-view-id>/views/dish_tables.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/index.xhtml</display-name>
		<from-view-id>/views/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToAds</from-outcome>
			<to-view-id>/views/ads.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/menu.xhtml</display-name>
		<from-view-id>/views/menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToIndex</from-outcome>
			<to-view-id>/views/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<lifecycle>
		<phase-listener>com.liferay.faces.util.lifecycle.DebugPhaseListener</phase-listener>
	</lifecycle>
</faces-config>